#:kivy 2.0.0
<DepartmentLevel>
    Background:
    Navbar:
    FloatLayout:
        size: root.width, root.height
        Button:
            text: "offer VS company"
            on_press: root.pie_chart()
            pos_hint: {'center_x': .3,'center_y': .5}
            size_hint: (0.2,0.2)
        Button:
            text: "year VS no.of students"
            on_press: root.double_bar()
            pos_hint: {'center_x': .5,'center_y': .5}
            size_hint: (0.2,0.2)
        Button:
            text: "year VS no.of students"
            on_press: root.multiple_bar()
            pos_hint: {'center_x': .7,'center_y': .5}
            size_hint: (0.2,0.2)
        Button:
            text: "Package vs Count"
            on_press: root.line_plot()
            pos_hint: {'center_x': .9,'center_y': .5}
            size_hint: (0.2,0.2)
