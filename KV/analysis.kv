#:kivy 2.0.0
<Analysis>
    Background:
    Navbar:
    MDLabel:
        halign: 'center'
        theme_text_color: "Custom"
        text_color: 0, 150/255, 136/255, 1
        text: "Analysis"
        font_style: 'H6'
        padding: (10,15)
        size_hint_y: None
        height: self.texture_size[1]
        font_size:30
        pos_hint:{"y":0.75}
    MDCard:
        size_hint: (0.25,0.4)
        pos_hint: {'center_x': .2, 'center_y': .4}
        elevation: 20
        orientation: 'vertical'
        padding: 22
        spacing: 30
        MDIconButton:
            halign:"center"
            icon:"account"
            user_font_size:"100sp"
            md_bg_color: app.theme_cls.primary_light
            pos_hint:{"x":.075}
            on_press:root.manager.current="view_eresources"
            on_press: root.manager.stack.append(root.name)
        MDRectangleFlatButton:
            halign:"center"
            text:"Indivisual Level"
            font_size:20
            #pos_hint:{"x":.02}
            on_press:root.manager.current="view_eresources"
            on_press: root.manager.stack.append(root.name)

    MDCard:
        size_hint: (0.25,0.4)
        pos_hint: {'center_x': .5, 'center_y': .4}
        elevation: 20
        orientation: 'vertical'
        padding: 25
        spacing: 30
        MDIconButton:
            icon:"google-classroom"
            user_font_size:"100sp"
            md_bg_color: app.theme_cls.primary_light
            pos_hint:{"x":.075}
            on_press:root.manager.current="view_assessments"
            on_press: root.manager.stack.append(root.name)
        MDRectangleFlatButton:
            text:"Dept Level"
            font_size:20
            pos_hint:{"x":.07}
            on_press:root.manager.current="view_assessments"
            on_press: root.manager.stack.append(root.name)
    MDCard:
        size_hint: (0.25,0.4)
        pos_hint: {'center_x': .8, 'center_y': .4}
        elevation: 20
        orientation: 'vertical'
        padding: 25
        spacing: 30
        MDIconButton:
            icon:"office-building-marker"
            user_font_size:"100sp"
            md_bg_color: app.theme_cls.primary_light
            pos_hint:{"x":.075}
            on_press:root.manager.current="placements"
            on_press: root.manager.stack.append(root.name)
        MDRectangleFlatButton:
            text:"Institute Level"
            font_size:20
            pos_hint:{"x":.04}
            on_press:root.manager.current="placements"
            on_press: root.manager.stack.append(root.name)
    